        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".AnaActivity" android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BaslatActivity" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".SehirSecActivity" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".IlceSecActivity" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".VakitIndirActivity" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".YanlisGittiActivity" android:theme="@style/Theme.AppCompat.NoActionBar"></activity>
    </application>

</manifest>